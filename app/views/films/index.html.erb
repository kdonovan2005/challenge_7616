<% @films.each do |result| %>
  <% year = result["year"] %>
  <% result["films"].each do |film| %>
    <% if film["Winner"] == true %>
      Title: <%= film["Film"] %> <br>
      <% url = film["Detail URL"] %>
      Budget: $<%= @film.get_budget(url) %><br>
      Year: <%= year %> <br> <br>
    <% end %>
  <% end %> <br>
<% end %>
